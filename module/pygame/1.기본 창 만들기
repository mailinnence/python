import pygame
from pickle import TRUE

#초기화
pygame.init()

#창 크기
WINDOW_WIDTH=900
WINDOW_HEIGHT=600

#창 구현
display_surface=pygame.display.set_mode((WINDOW_WIDTH,WINDOW_HEIGHT))

#창 제목
pygame.display.set_caption("game")

#프로그램이 실행하게 함
running =TRUE

#게임이 동작하는 동안 이벤트
while running:
    for event in pygame.event.get():
        if event.type == pygame.QUIT:
            running =False
            
            
pygame.quit()
    
